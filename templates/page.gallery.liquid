{% comment %}
  The contents of the page.gallery.liquid template can be found in /sections/gallery-section.liquid
{% endcomment %}

<div class="page-width page-custom-gallery">
  <div class="grid">
    <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
      <div class="section-header text-center">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        {{ page.content }}
      </div>

			<div class="rte">
				<div class="custom_gallery_collection">
					{% section 'gallery-section' %}
				</div>
        <div class="custom_gallery_collection custom_content">
          {% section 'custom-content' %}
        </div>
			</div>			

    </div>
  </div>
</div>
